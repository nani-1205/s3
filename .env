# Source AWS Account & S3 Details
SOURCE_AWS_ACCESS_KEY_ID="YOUR_SOURCE_ACCOUNT_ACCESS_KEY"
SOURCE_AWS_SECRET_ACCESS_KEY="YOUR_SOURCE_ACCOUNT_SECRET_KEY"
SOURCE_S3_REGION="ap-south-1"  # Assuming from your earlier request, adjust if needed
SOURCE_S3_BUCKET="bucket-name" # This is the bucket name visible in your breadcrumbs
SOURCE_S3_PREFIX="msi" # THIS IS THE KEY CHANGE

# Destination AWS Account & S3 Details
DEST_AWS_ACCESS_KEY_ID="YOUR_DEST_ACCOUNT_ACCESS_KEY"
DEST_AWS_SECRET_ACCESS_KEY="YOUR_DEST_ACCOUNT_SECRET_KEY"
DEST_S3_REGION="me-central-1" # Your specified destination region
DEST_S3_BUCKET="bucket-name" # Your specified destination bucket name
DEST_S3_PREFIX="mig" # You want to place the contents of source's mig/ into destination's mig/

# Flask specific
FLASK_APP=app.py
FLASK_DEBUG=True